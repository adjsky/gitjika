package pages

templ Layout(title string) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			<title>{ title }</title>
			<meta charset="utf-8"/>
			<meta name="viewport" content="width=device-width, initial-scale=1"/>
			<link rel="icon" href="/static/favicon.png"/>
			<link href="/static/styles.css" rel="stylesheet"/>
		</head>
		<body>
			{ children... }
		</body>
	</html>
}
